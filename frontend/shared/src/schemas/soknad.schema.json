{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/schemas/soknad.schema.json",
  "title": "SÃ¸knadsskjema",
  "type": "object",
  "properties": {
    "virksomhet": {
      "type": "object",
      "properties": {
        "navn": {
          "type": "string"
        },
        "virksomhetsnummer": {
          "type": "string"
        },
        "kontaktperson": {
          "type": "object",
          "properties": {
            "navn": {
              "type": "string"
            },
            "epost": {
              "type": "string"
            },
            "telefon": {
              "type": "string"
            }
          },
          "required": ["navn", "epost", "telefon"],
          "additionalProperties": false
        }
      },
      "required": ["navn", "virksomhetsnummer", "kontaktperson"],
      "additionalProperties": false
    },
    "ansatt": {
      "type": "object",
      "properties": {
        "fodselsnummer": {
          "type": "string"
        },
        "navn": {
          "type": "string"
        }
      },
      "required": ["fodselsnummer", "navn"],
      "additionalProperties": false
    },
    "ekspert": {
      "type": "object",
      "properties": {
        "navn": {
          "type": "string"
        },
        "virksomhet": {
          "type": "string"
        },
        "kompetanse": {
          "type": "string"
        },
        "problemstilling": {
          "type": "string"
        }
      },
      "required": ["navn", "virksomhet", "kompetanse", "problemstilling"],
      "additionalProperties": false
    },
    "bistand": {
      "type": "string"
    },
    "tiltak": {
      "type": "object",
      "properties": {
        "forTilrettelegging": {
          "type": "string"
        }
      },
      "required": ["forTilrettelegging"],
      "additionalProperties": false
    },
    "bestilling": {
      "type": "object",
      "properties": {
        "kostnad": {
          "oneOf": [{ "type": "number" }, { "type": "string" }]
        },
        "startDato": {
          "type": ["string", "null"],
          "format": "date-time"
        }
      },
      "required": ["kostnad", "startDato"],
      "additionalProperties": false
    },
    "nav": {
      "type": "object",
      "properties": {
        "kontakt": {
          "type": "string"
        }
      },
      "required": ["kontakt"],
      "additionalProperties": false
    }
  },
  "required": ["virksomhet", "ansatt", "ekspert", "bistand", "tiltak", "bestilling", "nav"],
  "additionalProperties": false
}
