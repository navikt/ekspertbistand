{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "lint": "eslint ../../frontend/client",
    "lint:fix": "eslint ../../frontend/client --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:cov": "vitest run --coverage",
    "test:run": "vitest run",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@grafana/faro-react": "1.19.0",
    "@grafana/faro-web-sdk": "1.19.0",
    "@hookform/resolvers": "5.2.2",
    "@navikt/aksel-icons": "7.33.2",
    "@navikt/ds-css": "7.33.2",
    "@navikt/ds-react": "7.33.2",
    "@navikt/nav-dekoratoren-moduler": "3.5.0",
    "@navikt/virksomhetsvelger": "1.0.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.66.0",
    "react-router-dom": "7.9.5",
    "shared": "workspace:*",
    "swr": "2.3.6",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react-swc": "4.2.2",
    "@vitest/coverage-v8": "4.0.8",
    "msw": "2.12.1",
    "typescript": "~5.9.3",
    "vite": "7.2.2",
    "vitest": "4.0.8",
    "whatwg-fetch": "3.6.20"
